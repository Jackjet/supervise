<!--我的工作台页面-->
<!DOCTYPE html>
<html>
	<head>
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
    	<meta http-equiv="content-type" content="text/html; charset=utf-8" />
		<title>我的工作台</title>
		<link rel="stylesheet" href="/df/portal/login/css/bootstrap.min.css" />
	</head>
	<body>
		<div class="col-md-12" style="width:100%;margin-top:10px;">
			<div class="col-md-3">
				<div class="col-md-12 panel panel-default" style="height:300px;border:1px solid #ccc;">
					<span style="float:left;">我的信息</span>
					<span style="float:right;"><a href="javascript:void(0);">更多</a></span>
					<hr style="width:100%;height:1px;border-top:1px solid blue;"/>
					<ul>
						<li style="list-style-type:none;">姓名：<span id="hp-name">xx</span></li>
						<li style="list-style-type:none;">部门：<span id="hp-department">xx</span></li>
						<li style="list-style-type:none;">电话：<span id="hp-num">15678942354</span></li>
						<li style="list-style-type:none;">系统角色：<span id="hp-role">xxx</span></li>
						<li style="list-style-type:none;">监管单位：<span id="hp-supunit">n家</span></li>
						<li style="list-style-type:none;">涉及资金：<span id="hp-funds">xx</span></li>
						<li style="list-style-type:none;">工作量：<span id="hp-workload">xx人天</span></li>
					</ul>
				</div>
				<div class="col-md-12 panel panel-default" style="height:300px;border:1px solid #ccc;margin-top:10px;">
					<span style="float:left;">我的联系人</span>
					<span style="float:right;"><a href="javascript:void(0);">更多</a></span>
					<hr style="width:100%;height:1px;border:none;border-top:1px solid blue;" />
					<!-- 查找 -->
					<div class="input-group">
						<input type="text" id="myInput" class="form-control" placeholder="查找">
						<icon > </icon>
					</div>
					<!--  -->
					
				</div>
			</div>
			<div class="col-md-6">
				<div class="col-md-12 panel panel-default" style="height:300px;border:1px solid #ccc;">
					<span style="float:left;">我的常用功能</span>
					<span style="float:right;padding:0 5px;">
						<a href="javascript:void(0);" data-bind="click:addCommonFunc">添加</a>
						<a href="javascript:void(0);" data-bind="click:commonFuncMore">更多</a>
					</span>
					<hr style="width:100%;height:1px;border:none;border-top:1px solid blue;" />
				</div>
				<div class="col-md-12" style="height:300px;margin-top:10px;padding:0;">
					<div class="col-md-6 panel panel-default" style="width:49%;height:300px;border:1px solid #ccc;">
						<span style="float:left;">我的待办事项</span>
						<span style="float:right;"><a href="javascript:void(0);" data-bind="click:moreEvent">更多</a></span>
						<hr style="width:100%;height:1px;border:none;border-top:1px solid blue;" />
						<ul id="hp-my-todolist">
							<li>xxxxxxxxxxxxxx</li>
							<li></li>
							<li>xxxxxxxxxxxxxxxx</li>
							<li></li>
							<li>xxxxxxxxxxxxxxxx</li>
							<li></li>
						</ul>
					</div>
					<div class="col-md-6 panel panel-default" style="width:49%;margin-left:2%;height:300px;border:1px solid #ccc;">
						<span style="float:left;">我的消息通知</span>
						<span style="float:right;"><a href="javascript:void(0);">更多</a></span>
						<hr style="width:100%;height:1px;border:none;border-top:1px solid blue;" />
						<ul id="hp-my-mes-notice">
							<li>xxxxxxxxxxxxxx</li>
							<li></li>
							<li>xxxxxxxxxxxxxxxx</li>
							<li></li>
							<li>xxxxxxxxxxxxxxxx</li>
							<li></li>
						</ul>
					</div>
				</div>	    
			</div>
			<div class="col-md-3">
				<div class="col-md-12 panel panel-default" style="height:150px;border:1px solid #ccc;">
					<button type="button" class="btn btn-primary" style="width:120px;margin-top:20px;">监管事项管理</button>
					<button type="button" class="btn btn-primary" style="width:120px;margin-top:20px;">预算监管执行</button>
					<button type="button" class="btn btn-primary" style="width:120px;margin-top:10px;">监管工作台账</button>
					<button type="button" class="btn btn-primary" style="width:120px;margin-top:10px;">单位信息库</button>
				</div>
				<div class="col-md-12 panel panel-default" style="height:200px;border:1px solid #ccc;margin-top:10px;">
					<span style="float:left;">最新公告</span>
					<span style="float:right;"><a href="javascript:void(0);">更多</a></span>
					<hr style="width:100%;height:1px;border:none;border-top:1px solid blue;" />
					<ul id="hp-latest-announce">
						<li>xxxxxxxxxxxxxx</li>
						<li></li>
						<li>xxxxxxxxxxxxxxxx</li>
						<li></li>
						<li>xxxxxxxxxxxxxxxx</li>
						<li></li>
					</ul>
				</div>
				<div class="col-md-12 panel panel-default" style="height:200px;border:1px solid #ccc;margin-top:10px;">
					<span style="float:left;">政策法规</span>
					<span style="float:right;"><a href="javascript:void(0);">更多</a></span>
					<hr style="width:100%;height:1px;border:none;border-top:1px solid blue;" />
					<ul id="hp-policies-regula">
						<li>xxxxxxxxxxxxxx</li>
						<li></li>
						<li>xxxxxxxxxxxxxxxx</li>
						<li></li>
						<li>xxxxxxxxxxxxxxxx</li>
						<li></li>
					</ul>
				</div>
			</div>
		</div>
		<!-- 引入js-->
		<script src="../../trd/requirejs/require.debug.js"></script>
		<script src="../../require.config.js"></script>
		<script src="/df/trd/jquery/jquery-1.12.3.min.js"></script>
		<script src="/df/supervise/homepage/homepage.js" type="text/javascript"></script>
	</body>
</html>
