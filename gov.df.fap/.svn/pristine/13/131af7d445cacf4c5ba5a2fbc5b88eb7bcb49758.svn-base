.csof-head{
    width: 100%;
    height: 60px;
    border : 1px solid #ccc;
    background : #ff8040;
    position: fixed;
}
.name {
    font-size: 22px;
    color: #FFFFFF;
    position: absolute;
    top: 15px;
    left:15px;
}
.opreations{
    position: absolute;
    right:5px;
    top:2px;
}
.opreations span a{
    margin: 0 5px;
    font-size: 12px;
    color: #ffffff;
}
.relevant-infomation{
    position: absolute;
    right:5px;
    bottom:2px;
    font-size: 12px;
    color: #FFFFFF;
}
.relevant-infomation span{
    margin: 0 5px;
}
.csof-leftpanel{
    position: fixed;
    top:60px;
    width: 8%;
    border: 1px solid #FF8040;
    min-height: 740px;
}
.csof-rightpanel{

}
.left-catalog-common{
    width: 100%;
    height: 50px;
    border: 1px solid #FF8040;
    font-size: 16px;
    color: #FF8040;
}
.csof-leftpanel div:hover{
    background: #ff6600;
}
.first-catalog{
    background: #ffffff;
}
.second-catalog{
    background: #ffffcc;
}
.third-catalog{
    background: #ffffcc;
}
.four-catalog{
    background: #ffffcc;
}
.five-catalog{
