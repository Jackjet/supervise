/*按钮样式*/
.csof-btn {
	background: #EDF5FF;
    border: 1px solid #99BCEB;
    border-radius: 5px;
    font-size: 12px;
    color: #4383DD;
    margin: 0 5px;
}
.btn.disabled, .btn[disabled], fieldset[disabled] .btn{
	background: #F9F9F9;
    border: 1px solid #D9D9D9;
    border-radius: 5px;
    color: #B3B3B3;
}
.btn:hover, .btn:focus, .btn.focus {
    background-color: rgba(0,0,1.8,0);
    color: #4383DD;
    text-decoration: none;
}
/*字体样式*/
.csof-font{
	font-family:"Microsoft YaHei",Arial,Helvetica,sans-serif,"宋体";
	font-size:13px;
}
select {
    height: 25px;
    border: 1px solid #ccc;
}
textarea {
	border: 1px solid #ccc;
    border-radius: 4px;
}
/*清除浮动*/
.clear{ clear:both}
/*li样式加上不换行*/
.csof-li {
	list-style: none;
    list-style-image: url(../csofimages/li.png);
	white-space:nowrap; 
    text-overflow:ellipsis; 
    overflow: hidden; 
}
/*页签样式*/
.nav-tabs>li.active>a, .nav-tabs>li.active>a:hover, .nav-tabs>li.active>a:focus {
    background-color: #fff;
    color: #555;
    border-radius: 4px 4px 0 0;
}
.nav-tabs {
    border-bottom: 1px solid #ddd;
}
.nav-tabs>li {
    margin-top: 2px;
}
.nav-tabs>li>a {
    background-color: #ddd;
}
label.text-right {
    height: 34px;
    line-height: 34px;
}