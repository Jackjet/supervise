package gov.df.fap.api.dictionary;

import java.util.List;

/**
 * 索引操作
 * @author 
 * @version 
 */
public interface Indexable {

	
	
	/**
	 * 通过索引取数据
	 * @param indexKey
	 * @return
	 */
	public List getCacheObjectByIndex(Object indexKey);
	
	/**
	 * 删除索引
	 * @param indexKey
	 */
	public void removeIndex(Object indexKey);
	
	/**
	 * 清空索引
	 */
	public void clearIndex();
	
}
