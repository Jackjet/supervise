/* 修改iuap组件样式 */
.u-pearls .u-step-number, .u-pearls .u-step-icon{
    width: 20px;
    height: 20px;
    line-height:20px;
}

.u-pearls .u-step:before, .u-pearls .u-step:after{
    top:10px;
    height: 2px;
}
.u-pearls .u-step-number{
    font-size: 15px;
}
.u-pearls .u-step-number{
    font-size: 14px;
}
.u-pearls .u-step-title{
    font-size: 14px;
}
.u-pearls {
    margin-bottom: 0;
}
.u-pearls .u-step.current .u-step-number, .u-pearls .u-step.current .u-step-icon{
    color: #1871C1;
    border-color: #1871c1;
    /*background-color: #1871C1;*/
}
.u-pearls .u-step.done .u-step-number, .u-pearls .u-step.done .u-step-icon{
    background-color: #1871C1;
    border-color: #1871c1;
}
.u-pearls .u-step.current:before, .u-pearls .u-step.current:after{
    background-color: #1871C1;
    border-color: #1871c1;
}
.u-grid-header-link{
	height: 2.7em;
}
#addModal .u-grid-content-div{
	overflow-x: hidden;
}
.ent-btn{
	width: 60px;
	border-radius: 4px;
    background-color: #108ee9;
    color: #fff;
    padding: 5px 6px;
}
.ent-btn:hover{
	background-color: #fff;
    color: #108ee9;
}
/* #1871C1 */

.treeNode{
	display: inline-block;
	margin-left: 8px;
	background-color: #108ee9;
	height: 18px;
    width: 18px;
    border-radius: 9px;
	text-align: center;
	color: #fff;
}