@charset "utf-8";
*{margin: 0; padding: 0;box-sizing: border-box;}
body{font-size: 12px;width: 100%;height: 100%;background: #f2f2f2;font-family:"Microsoft YaHei","SimSun";overflow:hidden;}
ul,ol,li{list-style: none;}
img{border: none;}
a{text-decoration: none;}

/*头部样式*/
#head{width: 100%;height: 50px;background: #1B8CF2;}
#head-in{width: 1280px;height: 50px;position: relative;margin: 0 auto;}
#h-logo{float: left;width: 230px;height: 49px;background-image: -webkit-linear-gradient(90deg, #1B8CF2 0%, #003B82 100%);background-image: linear-gradient(90deg, #1B8CF2 0%, #003B82 100%);opacity: 0.5;padding: 0.1px;}
#text{font-size: 18px;width: 198px;height: 21px;margin: 15px 0 0 16px;color: #FFFFFF;}
#h-nav{float: left;margin-left: 8px;margin-top: 15px;width: 686px;height: 34px;line-height: 34px;}
#h-nav ul li{float: left;margin-left: 15px;}
#h-nav ul li:hover{background: #1871C1;}
.bg{background: #1871C1;}
#h-nav ul li a{display: block;color: #FFFFFF;font-family: "MicrosoftYaHei";font-size: 14px;padding: 0 7px;}
#h-time{float: left;color: #FFFFFF;font-size: 14px;width: 172px;margin-top: 23px;}
#h-time img{width: 20px;height: 20px;display: inline-block;margin-right: 2px;}
#h-time span{display: inline-block;vertical-align:top;}
#h-btn{float: right;color: #FFFFFF;font-size: 14px;margin-right: 6px;cursor: pointer;}
#h-btn img{width: 25px;height: 25px;margin-top: 20px;display: inline-block;margin-right: 2px;}
#h-btn span{display: inline-block;vertical-align:top;margin-top: 23px;}
#h-btn #pic{width: 8px;height: 6px;background: url(../img/sanjiao.png) no-repeat center bottom;margin: 29px 3px 0 4px ;}
#hid-btn{position: absolute;top: 52px;left:1176px ;width: 93px;height: 96px;background: #FFFFFF;
box-shadow: 0px 2px 4px 0px rgba(0,0,0,0.50);display: none;}
#hid-btn a{width: 93px;height: 32px;border: 1px solid #f1f1f1;text-align: center;line-height: 30px;color: #333333;font-size: 13px;display: block;}
#hid-btn a:hover{width: 93px;height: 32px;background: #F5F5F5;}
/*以下隐藏部分*/
.menulist{width: 100%;/*height: 311px;*/position: absolute;top: 50px;left: 0;background:#F8F8F8;z-index: 9;display: none;}
#jiantou{position: absolute;bottom: 13px;left: 48%;}
/*消息管理平台*/
.menulist .menu{position: absolute;top: 0px;left:0px;display: none;width: 100%;background: #F8F8F8;z-index: 9;}
/*消息发送设置*/
.menulist .menu{position: absolute;top: 0px;left:0px;display: none;width: 100%;background: #F8F8F8;z-index: 9;}

/*总账配置规范下发*/
.menulist .menu{position: absolute;top: 0px;left:0px;width: 100%;display: none;z-index: 9;background: yellow;}
.menu .level-2{width: 1280px;height: 30px;margin:17px auto 13px;}
.menu .level-2 li{font-size: 14px;color: #666666;width: 110px;margin:0 8px;float: left;text-align: center;line-height: 30px;}
.menu .level-2 li .level-3 p{width: 110px;height: 30px;text-align: center;line-height: 30px;background: #CCCCCC;margin: 5px 8px;}
.menu .level-2 li .level-3 p:hover{background: #1871c1;}
.menu .level-2 li .level-3 p a{font-size: 14px;color: #FFFFFF;}
.menu .level-2 li .level-3 p:last-child{margin-bottom: 30px;}
/*快捷方式管理*/
.menulist .menu{position: absolute;top: 0px;left: 0;width: 100%;background:#f8f8f8;display: none;z-index: 9;}

/*以上隐藏部分*/
 /*头部样式结束*/
/*主要内容样式*/
#wrap{width: 1280px;height: 630px;margin: 0 auto;position: relative;}
#main{width: 100%;height: 630px;}
#left{width: 198px;height: 552px;/*float: left;margin: 10px 10px 0 10px;*/position: absolute;top:10px;left: 10px;background: #FFFFFF;
		border: 1px solid #E5E5E5;box-shadow: 0px 0px 3px 0px rgba(0,0,0,0.30);border-radius: 2px;}
#left #hd{width: 197px;height: 38px;background: url(../img/bg-l.png) no-repeat;background-size: 100%;position: relative;}
#left #hd p{font-size: 14px;color: #FFFFFF;width: 127px;height: 36px;line-height: 36px;text-align: center;}
#left #hd #set{position: absolute;left: 160px;top: 6px;}
#left #hd #ss{position: absolute;left: 180px;top: 6px;cursor: pointer;}
#list-l{width:197px;height: 514px;}
#list-l ul li{width: 70px;height: 70px;border-radius: 3px;margin: 13px;float: left;}
#list-l ul li:first-child{background: #f5f5f5;position: relative;}
#count{width: 18px;text-align: center;height: 18px;line-height: 18px;border-radius: 50%;background: #D0021B;position: absolute;top:-7px;left:56px;color: #FFFFFF;}
#list-l ul li img{margin: 10px 24px 6px 20px;}
#list-l ul li a{font-size: 14px;color: #666666;}
#list-l ul li a p{margin: 0px 8px 4px 6px;}
#center{width: 786px;height: 552px;/*float: left;margin-top: 10px;*/position: absolute;top:10px;left:218px;background: #FFFFFF;border: 1px solid #E5E5E5;box-shadow: 0px 0px 1px 0px rgba(0,0,0,0.30);border-radius: 2px;position: relative;}
#set1{position: absolute;left: 10px;top: 6px;width: 12px;height: 10px;background: url(../img/set1.png) no-repeat;cursor: pointer;z-index:9;}
#cen-1{width: 100%;height: 270px;border-bottom: 1px solid #E5E5E5;}

#right{width: 255px;height: 552px;/*float: right;margin: 10px 10px 0 10px;*/position: absolute;right: 10px;top: 10px;}

#part1{width: 255px;height: 260px;background: #FFFFFF;border: 1px solid #E5E5E5;box-shadow: 0px 0px 3px 0px rgba(0,0,0,0.30);border-radius: 2px;margin-bottom: 8px;}
#part1 #hd-r1,#part2 #hd-r2{width: 255px;height: 36px;background: url(../img/bg-rg.png) no-repeat;border: 1px solid #E5E5E5;border-radius: 3px 3px 0px 0px;position: relative;}
#part1 #hd-r1 p,#part2 #hd-r2 p{width: 127px;height: 34px;font-size: 14px;color: #FFFFFF;position: absolute;left: 12px;top: 8px;}
#part1 #hd-r1 a,#part2 #hd-r2 a{font-size: 13px;color: #FFFFFF;position: absolute;top: 11px;left: 198px;}
#part1 #list-r1 li,#part2 #list-r2 li{width: 255px;height: 36px;line-height: 36px;border: 1px solid #ededed;font-size: 14px;}
#part1 #list-r1 li:last-child{border: none;}
#part1 #list-r1 li .icon,#part2 #list-r2 li .icon{width: 3px;height: 3px;background: #f5a623;display: inline-block;margin: 16px 13px 0 18px;}
#part1 #list-r1 li a,#part2 #list-r2 li a{width: 179px;height: 36px;color: #333333;overflow: hidden; text-overflow:ellipsis; white-space: nowrap;display: inline-block;vertical-align: top;}
#part1 #list-r1 li a:hover{text-decoration: underline;}
#part2 #list-r2 li a:hover{text-decoration: underline;}
#part2{width: 255px;height: 284px;background: #FFFFFF;border: 1px solid #E5E5E5;box-shadow: 0px 0px 3px 0px rgba(0,0,0,0.30);border-radius: 2px;}
#part2 #list-r2 li:last-child{border: none;}
.c-red{color:red ;}
.hid{display: none;}
.active{display: block;}
.hid-btn-disp{display :block !important; z-index:2;}
.hid-btn-none{display :none !important; z-index:-1;}
/*主要内容结束*/
/*底部内容*/
#footer{width: 100%;height: 25px;background: #E7F3F9;box-shadow: 0px -2px 2px 0px rgba(0,0,0,0.10);position: fixed;bottom: 0px;left: 0;}
/*底部内容结束*/
/*fixed固定定位*/
.fix{position: fixed;top: 262px;right: 0px;}
.fix .f-bg{width: 36px;height: 36px;background: #9b9b9b;margin-bottom: 1px;}
.fix .f-bg a img{margin: 10px 0 0 10px;}
