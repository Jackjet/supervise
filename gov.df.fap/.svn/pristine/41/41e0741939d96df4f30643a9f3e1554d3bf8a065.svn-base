@charset "utf-8";
*{margin: 0; padding: 0;box-sizing: border-box;}
body{font-size: 12px;width: 100%;height: 100%;background: #EFF3F6;font-family:"Microsoft YaHei","SimSun";overflow-x:auto;overflow-y: auto;}
ul,ol,li{list-style: none;}
img{border: none;}
a{text-decoration: none;}
.clearfix{clear:both;display:block; overflow:hidden; visibility:hidden; width:0px; height:0px;}

#box{width: 100%;min-width: 1270px;/*margin: 10px 15px 15px 12px;*/background: #F2F2F2;position: relative;height: 800px;}
#left{/*width:100%;*/height: 700px;/*float: left;*/margin: 10px 378px 10px 10px;/*position: absolute;top: 10px;left: 12px;*/}
#left .cen-1{width: 100%;height: 86px;background: #FFFFFF;border: 1px solid #D9D9D9;border-radius: 4px;margin-bottom: 10px;}
#left .cen-1 .list{width: 100%;height: 66px;padding-top: 20px;}
#left .cen-1 .list li{float: left;position: relative;}
#left .cen-1 .list>li:first-child{width: 8%;text-align: center;height: 50px;border-right:1px solid #E9E9E9;margin-left: 0 !important;}
#left .cen-1 .list>li:first-child>a{width: 32px;height: 40px;font-family: "微软雅黑";font-size: 16px;color: #333333;line-height: 20px;}
#left .cen-1 .list li a img{display: inline-block;margin: 10px 20px 0 20px;}
#left .cen-1 .list li a span{position: absolute;top: 16px;left: 66px;font-family: "微软雅黑";font-size: 16px;color: #333333;}
#left .cen-1 .icon{width: 20%;height: 50px;background: #FFFFFF;border: 1px solid #E9E9E9;border-radius: 4px;margin-left: 2%;}
.hidContent{width: 100%;height: 220px;display: none;padding-top: 10px;background: transparent !important;}
.hidContent>ul{width: 100%;height: 210px;background: #FFFFFF;border: 1px solid #D9D9D9;border-radius: 3px 0 0 0 3px 3px 3px;}
.hidContent>ul>li{width:100%;height: 30px;line-height: 30px;border-bottom: 1px dashed #E9E9E9;}
.hidContent>ul>li:last-child{border-bottom: none;}
.hidContent>ul>li>a{font-size: 14px;color: #333333;font-family: "微软雅黑";display: inline-block;}
.icon2{width: 3px;height: 3px;display: inline-block;background: #4B5F73;margin: 10px 13px 0px 15px;}

#left .part{width: 100%;height: 236px;/*background: greenyellow;*/margin-bottom: 10px;}
.part1{width: 49.2%;height: 100%;background: #FFFFFF;border: 1px solid #D9D9D9;border-radius: 4px;margin-right: 10px;float: left;}
.head-p1{width: 100%;height: 35px;border-bottom: 1px solid #E9E9E9;}
.head-p1>span{width: 170px;padding-left:20px;height: 35px;line-height:35px;display: inline-block;font-family: "微软雅黑";font-size: 16px;color: #FFFFFF;background: url(../../img/GKJB/zb-bg.png) no-repeat left center;float: left;}
#danwei-p1{float: right;margin: 12px 10px 0 0;font-size: 12px;color: #333333;font-family: "微软雅黑";}



.part2{width: 49.2%;height: 100%;background: #FFFFFF;border: 1px solid #D9D9D9;border-radius: 4px;float: right;}
.head-p2{width: 100%;height: 35px;border-bottom: 1px solid #E9E9E9;}
.head-p2>span{width: 170px;padding-left:20px;height: 35px;line-height:35px;display: inline-block;font-family: "微软雅黑";font-size: 16px;color: #FFFFFF;background: url(../../img/GKJB/tit-bg.png) no-repeat left center;float: left;}
#content-p2 ul{padding: 12px 0 0 16px;}
#content-p2 ul li{height: 28px;line-height: 28px;}
#content-p2 ul li a{color: #333333;font-family: "微软雅黑";font-size: 14px;display: inline-block;width: 310px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
#content-p2 ul li a:hover{text-decoration: none;}
.icon-p{width: 3px;height: 3px;display: inline-block;background: #F6A623;margin:0px 13px 15px 0px;}


#left .cen-2{width: 100%;height: 206px;background: #FFFFFF;border: 1px solid #D9D9D9;border-radius: 4px;margin-bottom: 10px;}
.head-l{width: 100%;height: 35px;border-bottom: 1px solid #E9E9E9;}
.head-l>span{width: 170px;padding-left:20px;height: 35px;line-height:35px;display: inline-block;font-family: "微软雅黑";font-size: 16px;color: #FFFFFF;background: url(../../img/GKJB/zb-bg.png) no-repeat left center;float: left;}
.selectDiv{float: right;}
.selectDiv input{width: 84px;height: 26px;background: #FFFFFF;border: 1px solid #D9D9D9;border-radius: 4px;margin: 5px 10px 0 5px;}
.selectDiv select{width: 84px;height: 26px;background: #FFFFFF;border: 1px solid #D9D9D9;border-radius: 4px;margin: 5px 10px 0 5px;}
.zhibiao{margin: 12px 0 0 20px;font-family: "微软雅黑";font-size: 14px;color: #333333;}
.zfjenum{color: #7DC338;}
.zbyenum{color:#F8A23C ;}
.zbje{margin-right: 20px;}
.zfje{margin-right: 20px;}
.danweiType{position: absolute;top: 0;right: 30px;}
#left .cen-3{width: 100%;height: 272px;background: #FFFFFF;border: 1px solid #D9D9D9;border-radius: 4px;}
.head-3{width: 100%;height: 35px;border-bottom: 1px solid #E9E9E9;}
.head-3>span{width: 170px;padding-left:20px;height: 35px;line-height:35px;display: inline-block;font-family: "微软雅黑";font-size: 16px;color: #FFFFFF;background: url(../../img/GKJB/zb-bg.png) no-repeat left center;float: left;}


#right{width: 358px;height: auto;/*background: yellow;*/float: right;margin: 0px 10px 10px 0;/*position: absolute;top: 10px;right: 10px;*/}
#search{width: 100%;padding-left: 6px;height: 41px;margin-bottom: 10px;}
#search #text{display: inline-block;border-radius: 3px;height: 41px;background: #FFFFFF;border: 1px solid #D9D9D9;position: relative;}
#search #text>input{padding-left: 8px;background:none;border: none;width: 343px;height: 100%;display: inline-block;color: rgba(153,153,153,0.50);font-family: MicrosoftYaHei;font-size: 14px;line-height: 18px;outline: none;}
#search #pic{width: 48px;height: 40px;border-left: 1px solid #D9D9D9;position: absolute;top: 0;right: 0;}
#search #pic>img{width: 14px;height: 14px;margin: 12px 0 0 15px;}
#right .cen1{width: 100%;height: 246px;background: #FFFFFF;border: 1px solid #D9D9D9;border-radius: 4px;margin-bottom: 10px;}
.head-r{width: 100%;height: 36.5px;border-bottom: 2px solid #108EE9;}
.head-r>span{width: 107px;text-align: center;line-height: 30px;height: 30px;display: inline-block;font-family: "微软雅黑";font-size: 14px;margin: 5.5px 10px 0 0;border-top-left-radius: 4px;
    border-top-right-radius: 4px;background: #E9E9E9;color: #333333;}
.head-r>span:first-child{margin-left: 18px;}
 
.biao{padding: 10px;font-size: 14px;color: #333333;}
.biao table tr{height: 30px;line-height: 30px;}
.biao table tr:nth-child(2n+1){background: #F2F2F2;}
.cen1 .biao table tr td{text-align: center;border: 1px solid #E9E9E9;}

.ac{background: #108EE9 !important;color: #FFFFFF !important;}
.hidden{display: none;}

#right .cen2{width: 100%;height: 364px;background: #FFFFFF;border: 1px solid #D9D9D9;border-radius: 4px;margin-bottom: 10px;}
.head-r22{width: 100%;height: 36.5px;border-bottom: 2px solid #108EE9;}
.head-r22>span{width: 45px;text-align: center;line-height: 30px;height: 30px;display: inline-block;font-family: "微软雅黑";font-size: 14px;margin: 5px 10px 0 0;border-top-left-radius: 4px;
    border-top-right-radius: 4px;background: #E9E9E9;color: #333333;}
.head-r22>span:first-child{margin-left: 18px;}
.cen2 .biao table tr td:first-child{padding-left: 6px;}
.cen2 .biao table tr td:nth-child(2){text-align: center;}
.cen2 .biao table tr td:nth-child(3){padding-right: 6px;}
.cen2 .biao table tr{height: 31px;line-height: 31px;}

#right .cen3{width: 100%;height: 148px;background: #FFFFFF;border: 1px solid #D9D9D9;border-radius: 4px;}
.head-r2{width: 100%;height: 30px;border-bottom: 1px solid #E9E9E9;}
.head-r2>span{float: left;width: 114px;height: 30px;padding-left: 10px;line-height: 30px;display: inline-block;color: #FFFFFF;font-size: 14px;font-family: "微软雅黑";background: url(../../img/GKJB/zf-bg.png) no-repeat left center;}
.head-r2>img{float: right;margin: 10px;}
#m-content>ul{margin: 6px 0 0 18px;}
.icon1{width: 3px;height: 3px;display: inline-block;background: #F6A623;margin:0px 13px 15px 0px;}
#m-content ul li{height: 30px;line-height: 30px;}
#m-content ul li a{color: #151515;font-family: "微软雅黑";font-size: 14px;display: inline-block;width: 267px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
#m-content ul li a:hover{color: #108EE9;text-decoration: none;}
.c-red{color: #F8A23C;margin-left: 13px;}
.c{color: #F8A23C;}

