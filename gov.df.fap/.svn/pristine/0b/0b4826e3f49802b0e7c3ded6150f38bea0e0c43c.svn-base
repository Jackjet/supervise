	<script type="text/javascript">
		$(document).ready(function(){
			$(".lubo").lubo({
			 }); 
			//提示
		    $('.hint').on('click',function(){
				$(this).hide();
			})
			$('.user').on('blur',function(){
				if($('.user').val() == ''){
					$('.hint-user').show();
				}
			}).on('focus',function(){
				$('.hint-user').hide();
			});
			$('.pwd').on('blur',function(){
				if($('.pwd').val() == ""){
					$('.hint-pwd').show();
				}
			}).on('focus',function(){
				$('.hint-pwd').hide();
			});
		});
	</script> 
