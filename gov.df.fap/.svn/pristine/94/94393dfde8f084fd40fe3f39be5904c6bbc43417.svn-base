package gov.df.fap.bean.portal;

import java.io.Serializable;

/**
 * 常用操作非持久bean
 * <p>AP_LINK</p>
 */
public class ApLinkBean implements Serializable  {

  private static final long serialVersionUID = -4129263706981148301L;

  private String guid;
  
  /**
   * 标题 menu.title
   */
  private String link_title;
  
  /**
   * 连接 menu.url
   */
  private String link_url;
  
  /**
   * 图片路径 img.path
   */
  private String link_img;
  
  /**
   * 类型，默认0<br>
   * <p>0 图片类型</p>
   * <p>1 文字类型</p>
   * <p>2 图文类型</p>
   */
  private String link_type;
  
  /**
   * 对应菜单id menu.guid
   */
  private String menu_id;
  
  /**
   * 排序值，值越小越靠前，默认0
   */
  private Integer ord_index;

  /**
   * 创建者用户名 user.name
   */
  private String create_user;
  
  /**
   * 创建时间 YYYY-MM-DD HH24:MI:SS
   */
  private String create_time;

  public ApLinkBean() {
  }

  public ApLinkBean(String guid, String link_title, String link_url, String link_img,
    String link_type, String menu_id, Integer ord_index, String create_user,
    String create_time) {
    super();
    this.guid = guid;
    this.link_title = link_title;
    this.link_url = link_url;
    this.link_img = link_img;
    this.link_type = link_type;
    this.menu_id = menu_id;
    this.ord_index = ord_index;
    this.create_user = create_user;
    this.create_time = create_time;
  }

  public String getGuid() {
    return guid;
  }

  public void setGuid(String guid) {
    this.guid = guid;
  }

  public String getLink_title() {
    return link_title;
  }

  public void setLink_title(String link_title) {
    this.link_title = link_title;
  }

  public String getLink_url() {
    return link_url;
  }

  public void setLink_url(String link_url) {
    this.link_url = link_url;
  }

  public String getLink_img() {
    return link_img;
  }

  public void setLink_img(String link_img) {
    this.link_img = link_img;
  }

  public String getLink_type() {
    return link_type;
  }

  public void setLink_type(String link_type) {
    this.link_type = link_type;
  }

  public String getMenu_id() {
    return menu_id;
  }

  public void setMenu_id(String menu_id) {
    this.menu_id = menu_id;
  }

  public Integer getOrd_index() {
    return ord_index;
  }

  public void setOrd_index(Integer ord_index) {
    this.ord_index = ord_index;
  }

  public String getCreate_user() {
    return create_user;
  }

  public void setCreate_user(String create_user) {
    this.create_user = create_user;
  }

  public String getCreate_time() {
    return create_time;
  }

  public void setCreate_time(String create_time) {
    this.create_time = create_time;
  }
  
}
