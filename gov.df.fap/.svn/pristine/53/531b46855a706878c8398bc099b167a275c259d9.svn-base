package gov.df.fap.service.message;

import java.io.Serializable;

public class MsgCellObject implements Serializable {
  private static final long serialVersionUID = 2L;

  private boolean isReaded = true;

  private String value = "";

  private boolean isSendUser = false;

  public MsgCellObject(String value, boolean isReaded) {
    this.value = value;
    this.isReaded = isReaded;
  }

  public boolean isReaded() {
    return isReaded;
  }

  public void setReaded(boolean isReaded) {
    this.isReaded = isReaded;
  }

  public String getValue() {
    return value;
  }

  public void setValue(String value) {
    this.value = value;
  }

  public String toString() {
    return value.toString();
  }

  public boolean isSendUser() {
    return isSendUser;
  }

  public void setSendUser(boolean isSendUser) {
    this.isSendUser = isSendUser;
  }
}
