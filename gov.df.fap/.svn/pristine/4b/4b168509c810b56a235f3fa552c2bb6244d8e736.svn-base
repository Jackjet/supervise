package gov.df.fap.util.exception;

public class FAppException extends FGeneralException{

	private static final long serialVersionUID = 1L;

	public FAppException() {
		
		super();
		
	}
	
	public FAppException(String code) {
		
		super(code);
		
	}
	
	public FAppException(String code,String message) {
		
		super(code,message);
		
	}
	
	public FAppException(Exception e) {
		
		super(e.getMessage(), e);
		
	}
	
}
