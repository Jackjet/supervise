#fileList{
    border-top: 1px solid #5eadee;
    width:100%;
    min-height: 100px;
    margin-top: 40px;
    padding-top: 10px;
}
#fileList > div{
    padding-top: 10px;
    padding-bottom: 45px;
}

.file-item{
    width: 100%;
    min-height: 40px;
    border-bottom: 1px solid #ccc;
}

.webuploader-pick{
    padding: 0;
    background-color: #108ee9;
}
.info{
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: normal;

}
.fileName{
    height: 17px;
    margin-bottom: 5px;
}
.fileSize{
    padding-left: 0;
}

.state,.dealWith,.checkBox{
    margin-top: 10px;
}
.icon{
    /* margin-right: 20px; */
    cursor: pointer;
}
.icon:hover{
    text-decoration: underline;
 }
.dealWith > div{
    display: inline-block;
}

 .preShow-body{
     height: 400px;
     margin: 10px auto;
     overflow: auto;
 }
 #preShowCon{

 }
 .deleteItem{
     display: inline-block;
 }

 /* 状态颜色   成功     失败 */
.failed{
    color: red;
}

.success{
    color: #0aa151;
}