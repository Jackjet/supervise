	        		var selectNodeId = nodes[0].CHR_ID;
	        		$.get(getExURL, {"sid":selectNodeId}, function(result){
	        			var data = result.data;
	        			if(data[0].WORK_TYPE == "0"){
	        				var workType = "财政部布置";
	        			} else if(data[0].WORK_TYPE == "1"){
	        				var workType = "自行安排";
	        			} else{
	        				var workType = "";
	        			}
	        			switch(data[0].SUP_MODE){
	        			case "0" : 
	        				var supMode = "审核";
	        				break;
	        			case "1" :
	        				var supMode = "备查";
	        				break;
	        			case "2" :
	        				var supMode = "核查";
	        				break;
	        			case "3" :
