package gov.df.fap.service.util.gl.coa.cascade;

import java.util.List;

/**
 * 级联COA DTO
 * @author LiuYan
 * @version Mar 21, 2011
 */
public class CoaRelation implements java.io.Serializable {

  private static final long serialVersionUID = 1L;

  private List relationCoaList = null;

  //关联Coa是否分叉
  private boolean isBranch = false;

  public boolean isBranch() {
    return isBranch;
  }

  public void setIsBranch(boolean isBranch) {
    this.isBranch = isBranch;
  }

  public List getRelationCoaList() {
    return relationCoaList;
  }

  public void setRelationCoaList(List relationCoaList) {
    this.relationCoaList = relationCoaList;
  }

}
