package gov.df.fap.service.util.exceptions.workflow;

import java.sql.SQLException;

/**
 * 工作流
 * sql操作异常
 *
 * @author  
 * @version 
 * @since   java 1.6
 */
public class WFIllegalSqlOperateException extends SQLException{
    /**
	 * 
	 */
	private static final long serialVersionUID = 4131168983427890617L;

	/**
     * Constructs an <code>WFIllegalSqlOperatException</code> with no 
     * detail message. 
     */
    public WFIllegalSqlOperateException() {
	super();
    }

    /**
     * Constructs an <code>WFIllegalSqlOperatException</code> with the 
     * specified detail message. 
     *
     * @param   s   the detail message.
     */
    public WFIllegalSqlOperateException(String s) {
	super(s);
    }
}
