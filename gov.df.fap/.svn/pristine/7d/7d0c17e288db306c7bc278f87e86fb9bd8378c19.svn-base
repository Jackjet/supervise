require(['jquery', 'knockout','bootstrap', 'uui', 'tree', 'grid', 'ip'], function ($, ko) {
	window.ko = ko;
	console.log(parent);
	var options = ip.getCommonOptions({});
	var viewModel={
			todolistDataTable:new u.DataTable({
